{
	"name": "@traek/elements",
	"version": "0.0.3",
	"description": "Framework-agnostic mount API for traek — use in vanilla JS, React, Vue, or any framework",
	"author": "Nico Hülscher",
	"license": "MIT",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./styles.css": "./dist/index.css"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"scripts": {
		"build": "vite build && cp src/index.d.ts dist/index.d.ts",
		"check": "echo 'types checked during build'",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write . && eslint . --fix",
		"clean": "rm -rf node_modules dist .turbo",
		"test": "echo 'no tests yet'"
	},
	"dependencies": {
		"svelte": "^5.49.2",
		"traek": "workspace:*"
	},
	"devDependencies": {
		"@eslint/compat": "2.0.2",
		"@eslint/js": "9.39.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-svelte": "3.14.0",
		"globals": "17.3.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0",
		"vite": "^7.3.1"
	},
	"keywords": [
		"traek",
		"web-components",
		"custom-elements",
		"ai",
		"chat",
		"tree",
		"vanilla-js"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gettraek/traek.git",
		"directory": "packages/elements"
	}
}
